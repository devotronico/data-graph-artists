    "prettier": "^2.0.5",
    "prop-types": "^15.7.2"
  },
  "engines": {
    "node": "12.16.3"
  },
  "scripts": {
    "start": "cd server && npm run start",
    "build": "npm run build-client && npm run build-server",
    "dev": "concurrently \"cd server && npm run dev\" \"cd client && npm run start\" ",
    "test": "npm run test-client && npm run test-server",
    "coverage": "npm run coverage-client && npm run coverage-server",
    "lint": "npx eslint --fix **/*.js",
    "build-client": "cd client && npm install && npm run build",
    "build-server": "cd server && npm install",
    "test-client": "cd client && npm run test",
    "test-server": "cd server && npm run test",
    "coverage-client": "cd client && npm run coverage",
    "coverage-server": "cd server && npm run coverage"
  },
  "repository": {
